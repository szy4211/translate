{
    "name": "szy4211\/translate",
    "description": "A translate SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "zornshuai",
            "email": "zornshuai@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.5 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Szy4211\\Translate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szy4211\\Translate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Szy4211\\Translate\\TranslateServiceProvider"
            ]
        }
    }
}
